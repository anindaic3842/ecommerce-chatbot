{
    "source": {
      "include": ["src"],
      "exclude": ["node_modules"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)"
    },
    "opts": {
      "destination": "./docs/",
      "recurse": true
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }